{
  "name": "opennextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "cf-deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env=$DEPLOY_ENV",
    "cf-populate": "opennextjs-cloudflare populateCache remote --env=$DEPLOY_ENV",
    "cf-preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --env=$DEPLOY_ENV",
    "cf-typegen": "wrangler types --env=dev --env-interface CloudflareEnv src/types/cloudflare-env.d.ts",
    "ci": "opennextjs-cloudflare build --env=dev",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.3.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.539.0",
    "next": "15.3.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.3.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.2.0",
    "prettier": "^3.6.0",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-sort-json": "^4.1.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.4",
    "typescript": "^5",
    "wrangler": "^4.20.5"
  },
  "packageManager": "pnpm@10.14.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide",
      "sharp",
      "unrs-resolver",
      "workerd"
    ]
  }
}
