{
  "name": "opennextjs-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "cf-deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy --env=$DEPLOY_ENV",
    "cf-populate": "opennextjs-cloudflare populateCache remote --env=$DEPLOY_ENV",
    "cf-preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview --env=$DEPLOY_ENV",
    "cf-typegen": "wrangler types --env=dev --env-interface CloudflareEnv src/types/cloudflare-env.d.ts",
    "ci": "opennextjs-cloudflare build --env=dev",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "lint:md": "markdownlint *.md",
    "start": "next start"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.6.5",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "country-flag-icons": "^1.5.19",
    "idb": "^8.0.3",
    "lucide-react": "^0.553.0",
    "next": "^15.5.0",
    "next-intl": "^4.3.5",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.5.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.0.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-unicorn": "^61.0.0",
    "globals": "^16.3.0",
    "markdownlint-cli": "^0.46.0",
    "prettier": "^3.6.2",
    "prettier-plugin-multiline-arrays": "^4.0.3",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-packagejson": "^2.5.19",
    "prettier-plugin-sort-json": "^4.1.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "wrangler": "^4.31.0"
  },
  "packageManager": "pnpm@10.22.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "@tailwindcss/oxide",
      "sharp",
      "unrs-resolver",
      "workerd"
    ]
  }
}
